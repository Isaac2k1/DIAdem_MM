Class Layout
	
	private timeUnit
	private pageFormat
	private pageLogo
	private pageTitle
	
	Public Function setTimeUnit(VtimeUnit)
		timeUnit = VtimeUnit
	End Function
	
	Public Function getTimeUnit()
		getTimeUnit = timeUnit
	End Function
	
	Public Function setPageFormat(VpageFormat)
		pageFormat = VpageFormat
	End Function
	
	Public Function getPageFormat()
		getPageFormat = pageFormat
	End Function
	
	Public Function setPageLogo(VpageLogo)
		pageLogo = VpageLogo
	End Function
	
	Public Function getPageLogo()
		getPageLogo = pageLogo
	End Function
	
	Public Function setPageTitle(VpageTitle)
		pageTitle = VpageTitle
	End Function
	
	Public Function getPageTitle()
		getPageTitle = pageTitle
	End Function
	
End Class